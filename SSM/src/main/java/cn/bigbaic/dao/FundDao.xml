<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bigbaic.dao.FundDao">

<!--    &lt;!&ndash;这里可以写别名，是mybatis.xml里设置的resultType=Student&ndash;&gt;
    <select id="selectStudent" resultType="cn.bigbaic.domain.Student">
        select * from student by id desc;
    </select>-->
    <insert id="insetFund" >
        insert into fund(fundcode,name,jzrq,dwjz,gsz,gszzl,gztime)
        value(#{fundcode},#{name},#{jzrq},#{dwjz},#{gsz},#{gszzl},#{gztime})
    </insert>

</mapper>